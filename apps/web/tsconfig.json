{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@repo/database": ["../../packages/database/src"],
      "@repo/database/*": ["../../packages/database/src/*"],
  "@repo/ui": ["../../packages/ui/src"],
  "@repo/ui/*": ["../../packages/ui/src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "../admin/components/dashboard-summary.tsx", "../admin/components/dashboard-header.tsx", "../admin/components/user-avatar.tsx", "../../packages/ui/container.tsx", "src/scripts/import-products.js"  ],
  "exclude": [
    "node_modules"
  ]
}